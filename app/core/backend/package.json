{
  "name": "backend",
  "version": "0.0.1",
  "description": "Fastify backend for ft_transcendence",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
